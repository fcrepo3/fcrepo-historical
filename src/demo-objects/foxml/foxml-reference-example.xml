			<foxml:serviceInputMap>
				<foxml:datastreamBinding KEY="THUMBRES_IMG" LABEL="thumbnail image as datastream input to service" ORDER="0" DATASTREAM_ID="DS1"/>
				<foxml:datastreamBinding KEY="MEDRES_IMG" LABEL="medium res image as datastream input to service" ORDER="1" DATASTREAM_ID="DS2"/>
				<foxml:datastreamBinding KEY="HIGHRES_IMG" LABEL="high res image as datastream input to service" ORDER="2" DATASTREAM_ID="DS3"/>
				<foxml:datastreamBinding KEY="VERYHIGHRES_IMG" LABEL="master image as datastream input to service" ORDER="3" DATASTREAM_ID="DS4"/>
			</foxml:serviceInputMap>
		</foxml:disseminatorVersion>
