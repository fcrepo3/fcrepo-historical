	<!--- DISSEMINATOR 1:  UVA STANDARD IMAGE -->
	<foxml:disseminator ID="DISS1" BDEF_CONTRACT_PID="demo:1" STATE="A" VERSIONABLE="true">
		<foxml:disseminatorVersion ID="DISS1.0" LABEL="TIFF Converter Service for BDEF UVA-STD-IMG" BMECH_SERVICE_PID="demo:4" CREATED="2004-08-18T06:32:00">
			<!--- The first version of this disseminator fulfills the UVA Std Image (demo:1) behaviors by converting from a single mrsid  image -->
			<foxml:serviceInputMap>
				<foxml:datastreamBinding KEY="MRSID" LABEL="Binding to mrsid image" DATASTREAM_ID="MASTER"/>
			</foxml:serviceInputMap>
		</foxml:disseminatorVersion>
		<!--- The second version of this disseminator fulfills the UVA Std Image (demo:1) behaviors by returning the appropriate static image datastream -->
