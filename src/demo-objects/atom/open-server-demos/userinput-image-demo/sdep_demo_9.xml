  <entry xmlns:thr="http://purl.org/syndication/thread/1.0">
    <id>info:fedora/demo:9/METHODMAP/2008-04-30T03:54:32.108Z</id>
    <title type="text">METHODMAP1.0</title>
    <updated>2008-04-30T03:54:32.108Z</updated>
    <thr:in-reply-to ref="info:fedora/demo:9/METHODMAP"></thr:in-reply-to>
    <category term="Mapping of WSDL to Fedora notion of Method Definitions" scheme="info:fedora/fedora-system:def/model#label"></category>
    <category term="DISABLED" scheme="info:fedora/fedora-system:def/model#digestType"></category>
    <category term="none" scheme="info:fedora/fedora-system:def/model#digest"></category>
    <category term="1472" scheme="info:fedora/fedora-system:def/model#length"></category>
    <content type="text/xml">
      <fmm:MethodMap xmlns:fmm="http://fedora.comm.nsdlib.org/service/methodmap" bDefPID="demo:8" name="MethodMap - MrSID BMech for UserInput Image BDEF">
					
        <fmm:Method operationName="getThumbnail" wsdlMsgName="getThumbRequest" wsdlMsgOutput="mrsid_response">
						
          <fmm:DatastreamInputParm parmName="MRSID" passBy="URL_REF"></fmm:DatastreamInputParm>
						
          <fmm:MethodReturnType wsdlMsgName="mrsid_response" wsdlMsgTOMIME="image/gif"></fmm:MethodReturnType>
					</fmm:Method>
					
        <fmm:Method operationName="getImage" wsdlMsgName="getImageRequest" wsdlMsgOutput="mrsid_response">
						
          <fmm:UserInputParm defaultValue="yes" parmName="ZOOM" passBy="VALUE" required="true">
							
            <fmm:ValidParmValues>
								
              <fmm:ValidParm value="yes"></fmm:ValidParm>
								
              <fmm:ValidParm value="no"></fmm:ValidParm>
							</fmm:ValidParmValues>
						</fmm:UserInputParm>
						
          <fmm:UserInputParm defaultValue="medium1" parmName="SIZE" passBy="VALUE" required="true">
							
            <fmm:ValidParmValues>
								
              <fmm:ValidParm value="small"></fmm:ValidParm>
								
              <fmm:ValidParm value="medium1"></fmm:ValidParm>
								
              <fmm:ValidParm value="medium2"></fmm:ValidParm>
							</fmm:ValidParmValues>
						</fmm:UserInputParm>
						
          <fmm:DatastreamInputParm parmName="MRSID" passBy="URL_REF"></fmm:DatastreamInputParm>
						
          <fmm:MethodReturnType wsdlMsgName="mrsid_response" wsdlMsgTOMIME="application/java image/gif"></fmm:MethodReturnType>
					</fmm:Method>
				</fmm:MethodMap>
    </content>
  </entry>
