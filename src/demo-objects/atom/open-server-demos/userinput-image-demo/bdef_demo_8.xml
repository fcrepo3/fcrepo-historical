  <entry xmlns:thr="http://purl.org/syndication/thread/1.0">
    <id>info:fedora/demo:8/METHODMAP/2008-04-18T15:40:04.476Z</id>
    <title type="text">METHODMAP1.0</title>
    <updated>2008-04-18T15:40:04.476Z</updated>
    <thr:in-reply-to ref="info:fedora/demo:8/METHODMAP"></thr:in-reply-to>
    <category term="Method Map of the Fedora Behavior Definition for UserInput Image" scheme="info:fedora/fedora-system:def/model#label"></category>
    <category term="DISABLED" scheme="info:fedora/fedora-system:def/model#digestType"></category>
    <category term="none" scheme="info:fedora/fedora-system:def/model#digest"></category>
    <category term="905" scheme="info:fedora/fedora-system:def/model#length"></category>
    <content type="text/xml">
      <fmm:MethodMap xmlns:fmm="http://fedora.comm.nsdlib.org/service/methodmap" name="Fedora MethodMap for UserInput Image BDef">
					
        <fmm:Method operationName="getThumbnail"></fmm:Method>
					
        <fmm:Method operationName="getImage">
						
          <fmm:UserInputParm defaultValue="yes" parmName="ZOOM" passBy="VALUE" required="true">
							
            <fmm:ValidParmValues>
								
              <fmm:ValidParm value="yes"></fmm:ValidParm>
								
              <fmm:ValidParm value="no"></fmm:ValidParm>
							</fmm:ValidParmValues>
						</fmm:UserInputParm>
						
          <fmm:UserInputParm defaultValue="medium1" parmName="SIZE" passBy="VALUE" required="true">
							
            <fmm:ValidParmValues>
								
              <fmm:ValidParm value="small"></fmm:ValidParm>
								
              <fmm:ValidParm value="medium1"></fmm:ValidParm>
								
              <fmm:ValidParm value="medium2"></fmm:ValidParm>
							</fmm:ValidParmValues>
						</fmm:UserInputParm>
					</fmm:Method>
				</fmm:MethodMap>
    </content>
