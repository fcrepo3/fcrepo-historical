  <entry xmlns:thr="http://purl.org/syndication/thread/1.0">
    <id>info:fedora/demo:4/METHODMAP/2008-04-30T03:54:31.931Z</id>
    <title type="text">METHODMAP1.0</title>
    <updated>2008-04-30T03:54:31.931Z</updated>
    <thr:in-reply-to ref="info:fedora/demo:4/METHODMAP"></thr:in-reply-to>
    <category term="Mapping of WSDL to Fedora notion of Method Definitions" scheme="info:fedora/fedora-system:def/model#label"></category>
    <category term="DISABLED" scheme="info:fedora/fedora-system:def/model#digestType"></category>
    <category term="none" scheme="info:fedora/fedora-system:def/model#digest"></category>
    <category term="2464" scheme="info:fedora/fedora-system:def/model#length"></category>
    <content type="text/xml">
      <fmm:MethodMap xmlns:fmm="http://fedora.comm.nsdlib.org/service/methodmap" bDefPID="demo:1" name="MethodMap - MrSID BMech for UVA Simple Image BDEF">
					
        <fmm:Method operationName="getThumbnail" wsdlMsgName="getThumbRequest" wsdlMsgOutput="mrsid_response">
						
          <fmm:DefaultInputParm defaultValue="thumbnail" parmName="THUMBRES_SIZE" passBy="VALUE" required="true"></fmm:DefaultInputParm>
						
          <fmm:DatastreamInputParm parmName="MRSID" passBy="URL_REF" required="true"></fmm:DatastreamInputParm>
						
          <fmm:MethodReturnType wsdlMsgName="mrsid_response" wsdlMsgTOMIME="image/gif"></fmm:MethodReturnType>
					</fmm:Method>
					
        <fmm:Method operationName="getMedium" wsdlMsgName="getMedRequest" wsdlMsgOutput="mrsid_response">
						
          <fmm:DefaultInputParm defaultValue="no" parmName="ZOOM" passBy="VALUE" required="true"></fmm:DefaultInputParm>
						
          <fmm:DefaultInputParm defaultValue="small" parmName="MEDRES_SIZE" passBy="VALUE" required="true"></fmm:DefaultInputParm>
						
          <fmm:DatastreamInputParm parmName="MRSID" passBy="URL_REF" required="true"></fmm:DatastreamInputParm>
						
          <fmm:MethodReturnType wsdlMsgName="mrsid_response" wsdlMsgTOMIME="application/java image/gif"></fmm:MethodReturnType>
					</fmm:Method>
					
        <fmm:Method operationName="getHigh" wsdlMsgName="getHighRequest" wsdlMsgOutput="mrsid_response">
						
          <fmm:DefaultInputParm defaultValue="no" parmName="ZOOM" passBy="VALUE" required="true"></fmm:DefaultInputParm>
						
          <fmm:DefaultInputParm defaultValue="medium1" parmName="HIGHRES_SIZE" passBy="VALUE" required="true"></fmm:DefaultInputParm>
						
          <fmm:DatastreamInputParm parmName="MRSID" passBy="URL_REF" required="true"></fmm:DatastreamInputParm>
						
          <fmm:MethodReturnType wsdlMsgName="mrsid_response" wsdlMsgTOMIME="application/java image/gif"></fmm:MethodReturnType>
					</fmm:Method>
					
        <fmm:Method operationName="getVeryHigh" wsdlMsgName="getVeryHighRequest" wsdlMsgOutput="mrsid_response">
						
          <fmm:DefaultInputParm defaultValue="no" parmName="ZOOM" passBy="VALUE" required="true"></fmm:DefaultInputParm>
						
          <fmm:DefaultInputParm defaultValue="medium2" parmName="VERYHIGHRES_SIZE" passBy="VALUE" required="true"></fmm:DefaultInputParm>
						
          <fmm:DatastreamInputParm parmName="MRSID" passBy="URL_REF" required="true"></fmm:DatastreamInputParm>
						
          <fmm:MethodReturnType wsdlMsgName="mrsid_response" wsdlMsgTOMIME="application/java image/gif"></fmm:MethodReturnType>
					</fmm:Method>
				</fmm:MethodMap>
    </content>
  </entry>
