  <entry xmlns:thr="http://purl.org/syndication/thread/1.0">
    <id>info:fedora/demo:3/METHODMAP/2008-04-30T03:54:31.737Z</id>
    <title type="text">METHODMAP1.0</title>
    <updated>2008-04-30T03:54:31.737Z</updated>
    <thr:in-reply-to ref="info:fedora/demo:3/METHODMAP"></thr:in-reply-to>
    <category term="Mapping of WSDL to Fedora notion of Method Definitions" scheme="info:fedora/fedora-system:def/model#label"></category>
    <category term="DISABLED" scheme="info:fedora/fedora-system:def/model#digestType"></category>
    <category term="none" scheme="info:fedora/fedora-system:def/model#digest"></category>
    <category term="1570" scheme="info:fedora/fedora-system:def/model#length"></category>
    <content type="text/xml">
      <fmm:MethodMap xmlns:fmm="http://fedora.comm.nsdlib.org/service/methodmap" bDefPID="demo:1" name="MethodMap - UVA ImageZoomer BMech for Simple Image BDEF">
					
        <fmm:Method operationName="getThumbnail" wsdlMsgName="getThumbRequest" wsdlMsgOutput="image_response">
						
          <fmm:DatastreamInputParm parmName="LOWRES_IMG" passBy="URL_REF" required="true"></fmm:DatastreamInputParm>
						
          <fmm:MethodReturnType wsdlMsgName="image_response" wsdlMsgTOMIME="text/html"></fmm:MethodReturnType>
					</fmm:Method>
					
        <fmm:Method operationName="getMedium" wsdlMsgName="getMedRequest" wsdlMsgOutput="image_response">
						
          <fmm:DatastreamInputParm parmName="MEDRES_IMG" passBy="URL_REF" required="true"></fmm:DatastreamInputParm>
						
          <fmm:MethodReturnType wsdlMsgName="image_response" wsdlMsgTOMIME="text/html"></fmm:MethodReturnType>
					</fmm:Method>
					
        <fmm:Method operationName="getHigh" wsdlMsgName="getHighRequest" wsdlMsgOutput="image_response">
						
          <fmm:DatastreamInputParm parmName="HIGHRES_IMG" passBy="URL_REF" required="true"></fmm:DatastreamInputParm>
						
          <fmm:MethodReturnType wsdlMsgName="image_response" wsdlMsgTOMIME="text/html"></fmm:MethodReturnType>
					</fmm:Method>
					
        <fmm:Method operationName="getVeryHigh" wsdlMsgName="getVeryHighRequest" wsdlMsgOutput="image_response">
						
          <fmm:DatastreamInputParm parmName="VERYHIGHRES_IMG" passBy="URL_REF" required="true"></fmm:DatastreamInputParm>
						
          <fmm:MethodReturnType wsdlMsgName="image_response" wsdlMsgTOMIME="text/html"></fmm:MethodReturnType>
					</fmm:Method>
				</fmm:MethodMap>
    </content>
