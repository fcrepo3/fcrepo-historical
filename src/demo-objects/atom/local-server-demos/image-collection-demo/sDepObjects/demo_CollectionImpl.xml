  <entry xmlns:thr="http://purl.org/syndication/thread/1.0">
    <id>info:fedora/demo:CollectionImpl/WSDL/2008-04-30T03:54:30.881Z</id>
    <title type="text">WSDL1.0</title>
    <updated>2008-04-30T03:54:30.881Z</updated>
    <thr:in-reply-to ref="info:fedora/demo:CollectionImpl/WSDL"></thr:in-reply-to>
    <category term="WSDL definition of service" scheme="info:fedora/fedora-system:def/model#label"></category>
    <category term="DISABLED" scheme="info:fedora/fedora-system:def/model#digestType"></category>
    <category term="none" scheme="info:fedora/fedora-system:def/model#digest"></category>
    <category term="4243" scheme="info:fedora/fedora-system:def/model#length"></category>
    <content type="text/xml">
      <wsdl:definitions xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" xmlns:http="http://schemas.xmlsoap.org/wsdl/http/" xmlns:mime="http://schemas.xmlsoap.org/wsdl/mime/" xmlns:soapenc="http://schemas.xmlsoap.org/wsdl/soap/encoding" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap" xmlns:this="bmech" name="Image Collection" targetNamespace="bmech">
          
        <wsdl:types>
            
          <xsd:schema targetNamespace="bmech">
              
            <xsd:simpleType name="TYPEType">
                
              <xsd:restriction base="xsd:string"></xsd:restriction>
              </xsd:simpleType>
              
            <xsd:simpleType name="QUERYType">
                
              <xsd:restriction base="xsd:string"></xsd:restriction>
              </xsd:simpleType>
              
            <xsd:simpleType name="XSLTType">
                
              <xsd:restriction base="xsd:string"></xsd:restriction>
              </xsd:simpleType>
              
            <xsd:simpleType name="LANGType">
                
              <xsd:restriction base="xsd:string"></xsd:restriction>
              </xsd:simpleType>
              
            <xsd:simpleType name="LISTType">
                
              <xsd:restriction base="xsd:string"></xsd:restriction>
              </xsd:simpleType>
              
            <xsd:simpleType name="CLEAR_CACHEType">
                
              <xsd:restriction base="xsd:string"></xsd:restriction>
              </xsd:simpleType>
              
            <xsd:simpleType name="FORMATType">
                
              <xsd:restriction base="xsd:string"></xsd:restriction>
              </xsd:simpleType>
            </xsd:schema>
          </wsdl:types>
          
        <wsdl:message name="viewRequest">
            
          <wsdl:part name="XSLT" type="this:XSLTType"></wsdl:part>
            
          <wsdl:part name="LIST" type="this:LISTType"></wsdl:part>
            
          <wsdl:part name="CLEAR_CACHE" type="this:CLEAR_CACHEType"></wsdl:part>
          </wsdl:message>
          
        <wsdl:message name="listRequest">
            
          <wsdl:part name="TYPE" type="this:TYPEType"></wsdl:part>
            
          <wsdl:part name="QUERY" type="this:QUERYType"></wsdl:part>
            
          <wsdl:part name="LANG" type="this:LANGType"></wsdl:part>
            
          <wsdl:part name="FORMAT" type="this:FORMATType"></wsdl:part>
          </wsdl:message>
          
        <wsdl:message name="dissemResponse">
            
          <wsdl:part name="dissem" type="xsd:base64Binary"></wsdl:part>
          </wsdl:message>
          
        <wsdl:portType name="ImageCollectionPortType">
            
          <wsdl:operation name="view">
              
            <wsdl:input message="this:viewRequest"></wsdl:input>
              
            <wsdl:output message="this:dissemResponse"></wsdl:output>
            </wsdl:operation>
            
          <wsdl:operation name="list">
              
            <wsdl:input message="this:listRequest"></wsdl:input>
              
            <wsdl:output message="this:dissemResponse"></wsdl:output>
            </wsdl:operation>
          </wsdl:portType>
          
        <wsdl:service name="ImageCollection">
            
          <wsdl:port binding="this:ImageCollection_http" name="ImageCollection_port">
              
            <http:address location="http://localhost:8080/"></http:address>
            </wsdl:port>
          </wsdl:service>
          
        <wsdl:binding name="ImageCollection_http" type="this:ImageCollectionPortType">
            
          <http:binding verb="GET"></http:binding>
            
          <wsdl:operation name="view">
              
            <http:operation location="saxon/SaxonServlet?source=(LIST)&amp;style=(XSLT)&amp;clear-stylesheet-cache=(CLEAR_CACHE)"></http:operation>
              
            <wsdl:input>
                
              <http:urlReplacement></http:urlReplacement>
              </wsdl:input>
              
            <wsdl:output>
                
              <mime:content type="text/html"></mime:content>
              </wsdl:output>
            </wsdl:operation>
            
          <wsdl:operation name="list">
              
            <http:operation location="fedora/risearch?type=(TYPE)&amp;lang=(LANG)&amp;format=(FORMAT)&amp;query=(QUERY)"></http:operation>
              
            <wsdl:input>
                
              <http:urlReplacement></http:urlReplacement>
              </wsdl:input>
              
            <wsdl:output>
                
              <mime:content type="text/xml"></mime:content>
              </wsdl:output>
            </wsdl:operation>
          </wsdl:binding>
        </wsdl:definitions>
    </content>
  </entry>
