  <entry xmlns:thr="http://purl.org/syndication/thread/1.0">
    <id>info:fedora/demo:CollectionImpl/METHODMAP/2008-04-30T03:54:30.883Z</id>
    <title type="text">METHODMAP1.0</title>
    <updated>2008-04-30T03:54:30.883Z</updated>
    <thr:in-reply-to ref="info:fedora/demo:CollectionImpl/METHODMAP"></thr:in-reply-to>
    <category term="Mapping of WSDL to Fedora notion of Method Definitions" scheme="info:fedora/fedora-system:def/model#label"></category>
    <category term="DISABLED" scheme="info:fedora/fedora-system:def/model#digestType"></category>
    <category term="none" scheme="info:fedora/fedora-system:def/model#digest"></category>
    <category term="1864" scheme="info:fedora/fedora-system:def/model#length"></category>
    <content type="text/xml">
      <fmm:MethodMap xmlns:fmm="http://fedora.comm.nsdlib.org/service/methodmap" bDefPID="demo:Collection" name="MethodMap - Image Collection">
          
        <fmm:Method operationLabel="An html representation of the collection" operationName="view" wsdlMsgName="viewRequest" wsdlMsgOutput="dissemResponse">
            
          <fmm:DatastreamInputParm defaultValue="" label="Stylesheet to transform LIST into html" parmName="XSLT" passBy="URL_REF" required="true"></fmm:DatastreamInputParm>
            
          <fmm:DatastreamInputParm defaultValue="" label="The result of the list dissemination as a datastream" parmName="LIST" passBy="URL_REF" required="true"></fmm:DatastreamInputParm>
            
          <fmm:DefaultInputParm defaultValue="yes" label="Whether to reload the stylesheet each time" parmName="CLEAR_CACHE" passBy="VALUE" required="true"></fmm:DefaultInputParm>
            
          <fmm:MethodReturnType wsdlMsgName="dissemResponse" wsdlMsgTOMIME="text/html"></fmm:MethodReturnType>
          </fmm:Method>
          
        <fmm:Method operationLabel="An xml list of members in the collection" operationName="list" wsdlMsgName="listRequest" wsdlMsgOutput="dissemResponse">
            
          <fmm:DefaultInputParm defaultValue="tuples" label="" parmName="TYPE" passBy="VALUE" required="true"></fmm:DefaultInputParm>
            
          <fmm:DatastreamInputParm defaultValue="" label="" parmName="QUERY" passBy="URL_REF" required="true"></fmm:DatastreamInputParm>
            
          <fmm:DefaultInputParm defaultValue="itql" label="" parmName="LANG" passBy="VALUE" required="true"></fmm:DefaultInputParm>
            
          <fmm:DefaultInputParm defaultValue="Sparql" label="" parmName="FORMAT" passBy="VALUE" required="true"></fmm:DefaultInputParm>
            
          <fmm:MethodReturnType wsdlMsgName="dissemResponse" wsdlMsgTOMIME="text/xml"></fmm:MethodReturnType>
          </fmm:Method>
        </fmm:MethodMap>
    </content>
