    <category term="Mapping of WSDL to Fedora notion of Method Definitions" scheme="info:fedora/fedora-system:def/model#label"></category>
    <category term="DISABLED" scheme="info:fedora/fedora-system:def/model#digestType"></category>
    <category term="none" scheme="info:fedora/fedora-system:def/model#digest"></category>
    <category term="482" scheme="info:fedora/fedora-system:def/model#length"></category>
    <content type="text/xml">
      <fmm:MethodMap xmlns:fmm="http://fedora.comm.nsdlib.org/service/methodmap" bDefPID="demo:22" name="MethodMap - FO BDEF Methods">
					
        <fmm:Method operationName="getFO" wsdlMsgName="getFORequest" wsdlMsgOutput="response">
						
          <fmm:DatastreamInputParm parmName="TEI_SOURCE" passBy="URL_REF" required="true"></fmm:DatastreamInputParm>
						
          <fmm:MethodReturnType wsdlMsgName="response" wsdlMsgTOMIME="text/xml"></fmm:MethodReturnType>
					</fmm:Method>
				</fmm:MethodMap>
    </content>
