    <updated>2008-04-30T03:54:30.269Z</updated>
    <thr:in-reply-to ref="info:fedora/demo:20/METHODMAP"></thr:in-reply-to>
    <category term="Mapping of WSDL to Fedora notion of Method Definitions" scheme="info:fedora/fedora-system:def/model#label"></category>
    <category term="DISABLED" scheme="info:fedora/fedora-system:def/model#digestType"></category>
    <category term="none" scheme="info:fedora/fedora-system:def/model#digest"></category>
    <category term="485" scheme="info:fedora/fedora-system:def/model#length"></category>
    <content type="text/xml">
      <fmm:MethodMap xmlns:fmm="http://fedora.comm.nsdlib.org/service/methodmap" bDefPID="demo:19" name="MethodMap - PDF BDEF Methods">
					
        <fmm:Method operationName="getPDF" wsdlMsgName="getPDF" wsdlMsgOutput="response">
						
          <fmm:DatastreamInputParm parmName="XML_SOURCE" passBy="URL_REF" required="true"></fmm:DatastreamInputParm>
						
          <fmm:MethodReturnType wsdlMsgName="response" wsdlMsgTOMIME="application/pdf"></fmm:MethodReturnType>
					</fmm:Method>
				</fmm:MethodMap>
    </content>
