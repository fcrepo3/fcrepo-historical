    <foxml:datastream ID="METHODMAP" CONTROL_GROUP="X" STATE="A">
        <foxml:datastreamVersion ID="METHODMAP1.0" MIMETYPE="text/xml" CREATED="2000-08-22T06:32:00Z" LABEL="Method Map of the Fedora Behavior Definition for UserInput Image">
            <foxml:xmlContent>
                <fmm:MethodMap name="Fedora MethodMap for UserInput Image BDef" xmlns:fmm="http://fedora.comm.nsdlib.org/service/methodmap">
                    <fmm:Method operationName="getThumbnail"/>
                    <fmm:Method operationName="getImage">
                        <fmm:UserInputParm defaultValue="yes" parmName="ZOOM" passBy="VALUE" required="true">
                            <fmm:ValidParmValues>
                                <fmm:ValidParm value="yes"/>
                                <fmm:ValidParm value="no"/>
                            </fmm:ValidParmValues>
                        </fmm:UserInputParm>
                        <fmm:UserInputParm defaultValue="medium1" parmName="SIZE" passBy="VALUE" required="true">
                            <fmm:ValidParmValues>
                                <fmm:ValidParm value="small"/>
                                <fmm:ValidParm value="medium1"/>
                                <fmm:ValidParm value="medium2"/>
                            </fmm:ValidParmValues>
                        </fmm:UserInputParm>
                    </fmm:Method>
                </fmm:MethodMap>
            </foxml:xmlContent>
        </foxml:datastreamVersion>
    </foxml:datastream>
