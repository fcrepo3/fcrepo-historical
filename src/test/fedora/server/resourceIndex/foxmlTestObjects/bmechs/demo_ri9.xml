  <foxml:datastream CONTROL_GROUP="X" FORMAT_URI="info:fedora/fedora-system:format/xml.mets.techMD.OTHER.UNSPECIFIED"
    ID="METHODMAP" MIMETYPE="text/xml" STATE="A" VERSIONABLE="YES">
    <foxml:datastreamVersion CREATED="2002-02-17T06:32:00Z" ID="METHODMAP1.0"
      LABEL="Mapping of WSDL to Fedora notion of Method Definitions" SIZE="1510">
      <foxml:xmlContent>
        <fmm:MethodMap bDefPID="demo:8" name="MethodMap - MrSID BMech for UserInput Image BDEF" xmlns:fmm="http://fedora.comm.nsdlib.org/service/methodmap">
          <fmm:Method operationName="getThumbnail" wsdlMsgName="getThumbRequest" wsdlMsgOutput="mrsid_response">
            <fmm:DatastreamInputParm parmName="MRSID" passBy="URL_REF"/>
            <fmm:MethodReturnType wsdlMsgName="mrsid_response" wsdlMsgTOMIME="image/gif"/>
          </fmm:Method>
          <fmm:Method operationName="getImage" wsdlMsgName="getImageRequest" wsdlMsgOutput="mrsid_response">
            <fmm:UserInputParm defaultValue="yes" parmName="ZOOM" passBy="VALUE" required="true">
              <fmm:ValidParmValues>
                <fmm:ValidParm value="yes"/>
                <fmm:ValidParm value="no"/>
              </fmm:ValidParmValues>
            </fmm:UserInputParm>
            <fmm:UserInputParm defaultValue="medium1" parmName="SIZE" passBy="VALUE" required="true">
              <fmm:ValidParmValues>
                <fmm:ValidParm value="small"/>
                <fmm:ValidParm value="medium1"/>
                <fmm:ValidParm value="medium2"/>
              </fmm:ValidParmValues>
            </fmm:UserInputParm>
            <fmm:DatastreamInputParm parmName="MRSID" passBy="URL_REF"/>
            <fmm:MethodReturnType wsdlMsgName="mrsid_response" wsdlMsgTOMIME="application/java image/gif"/>
          </fmm:Method>
        </fmm:MethodMap>
      </foxml:xmlContent>
      <foxml:contentDigest DIGEST="future: hash of content goes here" TYPE="MD5"/>
    </foxml:datastreamVersion>
