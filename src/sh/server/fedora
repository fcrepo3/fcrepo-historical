#!/bin/sh

CLASSPATH=${FEDORA_HOME}/tomcat41/webapps/fedora/WEB-INF/classes:${FEDORA_HOME}/tomcat41/common/lib/xerces2-2.0.2.jar
java -classpath ${CLASSPATH} -Djavax.xml.parsers.DocumentBuilderFactory=org.apache.xerces.jaxp.DocumentBuilderFactoryImpl -Dfedora.home=${FEDORA_HOME} fedora.server.Server "$@"
